  pod 'SVGKit', :git => 'https://github.com/SVGKit/SVGKit.git', :branch => '2.x'
  pod 'Pulley'
  pod 'SwiftyJSON'

  target 'GMapsTestTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GMapsTestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
